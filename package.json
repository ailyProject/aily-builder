{
  "name": "aily-builder",
  "version": "1.0.0",
  "description": "Fast Arduino compilation CLI tool with optimized preprocessing and parallel compilation",
  "main": "dist/main.js",
  "bin": {
    "aily": "dist/main.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "bundle": "npm run build && node bundle.js",
    "bundle:minify": "npm run build && node bundle.js --minify",
    "bundle:both": "npm run build && node bundle.js --both",
    "bundle:native": "npm run build && node bundle-native.js",
    "bundle:native:minify": "npm run build && node bundle-native.js --minify",
    "bundle:native:both": "npm run build && node bundle-native.js --both",
    "package": "npm run build && node build.js",
    "start": "ts-node main.ts",
    "blink": "ts-node main.ts compile examples/blink.ino --verbose --jobs 4 --log-file",
    "sweep": "ts-node main.ts compile examples/sweep.ino --board arduino:renesas_uno:unor4wifi --jobs 4 --libraries-path \"C:\\Users\\coloz\\AppData\\Local\\Arduino15\\libraries\"",
    "tft": "ts-node main.ts compile examples/tft.ino --verbose --jobs 4 --libraries-path \"C:\\Users\\coloz\\AppData\\Local\\Arduino15\\libraries\"",
    "esp32": "ts-node main.ts compile examples/esp32-http.ino --jobs 4 --verbose --board esp32:esp32:esp32",
    "blink-r4": "ts-node main.ts compile examples/blink.ino --verbose --board arduino:renesas_uno:unor4wifi",
    "aivox": "ts-node main.ts compile \"C:\\Users\\coloz\\Documents\\aily-project\\AIVOXControlLed_aug28a\\.temp\\sketch\\sketch.ino\" --jobs 4 --board esp32:esp32:esp32s3 --libraries-path \"C:\\Users\\coloz\\Documents\\aily-project\\AIVOXControlLed_aug28a\\.temp\\libraries\"",
    "aivox2": "ts-node main.ts compile \"C:\\Users\\coloz\\Documents\\aily-project\\AIVOXSensor_sep03a\\.temp\\sketch\\sketch.ino\" --jobs 4 --verbose --board esp32:esp32:esp32s3 --libraries-path \"C:\\Users\\coloz\\Documents\\aily-project\\AIVOXSensor_sep03a\\.temp\\libraries\"",
    "test": "ts-node main.ts compile \"C:\\Users\\coloz\\Documents\\aily-project\\blinker_led_sep18a\\.temp\\sketch\\sketch.ino\" --jobs 6 --board esp32:esp32:esp32s3 --libraries-path \"C:\\Users\\coloz\\Documents\\aily-project\\blinker_led_sep18a\\.temp\\libraries\"",
    "test2": "ts-node main.ts compile \"C:\\Users\\coloz\\Documents\\aily-project\\project_sep21a\\.temp\\sketch\\sketch.ino\" --jobs 4 --board \"arduino:renesas_uno:unor4wifi\" --libraries-path \"C:\\Users\\coloz\\Documents\\aily-project\\project_sep21a\\.temp\\libraries\" --sdk-path \"C:\\Users\\coloz\\AppData\\Local\\aily-project\\sdk\\renesas_uno_1.5.1\" --tools-path \"C:\\Users\\coloz\\AppData\\Local\\aily-project\\tools\" --log-file --verbose",
    "clean": "rimraf dist"
  },
  "keywords": [
    "arduino",
    "cli",
    "compilation",
    "build",
    "embedded"
  ],
  "author": "coloz",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.5",
    "tree-sitter": "^0.20.4",
    "tree-sitter-cpp": "^0.20.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.16",
    "esbuild": "^0.25.9",
    "jest": "^29.7.0",
    "nexe": "^5.0.0-beta.4",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16"
  }
}
