{
  "name": "aily-builder",
  "version": "1.0.0",
  "description": "Fast Arduino compilation CLI tool with optimized preprocessing and parallel compilation",
  "main": "dist/main.js",
  "bin": {
    "aily": "dist/main.js"
  },
  "scripts": {
    "build": "tsc",
    "package": "npm run build && node build.js",
    "start": "ts-node main.ts",
    "blink": "ts-node main.ts compile examples/blink.ino --verbose --jobs 4 ",
    "sweep": "ts-node main.ts compile examples/sweep.ino --board arduino:renesas_uno:unor4wifi --jobs 4  --verbose --libraries-path \"C:\\Users\\coloz\\AppData\\Local\\Arduino15\\libraries\"",
    "tft": "ts-node main.ts compile examples/tft.ino --verbose --jobs 4 --libraries-path \"C:\\Users\\coloz\\AppData\\Local\\Arduino15\\libraries\"",
    "esp32": "ts-node main.ts compile examples/esp32-http.ino --jobs 4 --verbose --board esp32:esp32:esp32",
    "blink-r4": "ts-node main.ts compile examples/blink.ino --verbose --board arduino:renesas_uno:unor4wifi",
    "aivox": "ts-node main.ts compile \"C:\\Users\\coloz\\Documents\\aily-project\\AIVOXControlLed_aug28a\\.temp\\sketch\\sketch.ino\" --jobs 4 --verbose --board esp32:esp32:esp32s3 --libraries-path \"C:\\Users\\coloz\\Documents\\aily-project\\AIVOXControlLed_aug28a\\.temp\\libraries\"",
    "aivox2": "ts-node main.ts compile \"C:\\Users\\coloz\\Documents\\aily-project\\AIVOXSensor_sep03a\\.temp\\sketch\\sketch.ino\" --jobs 4 --verbose --board esp32:esp32:esp32s3 --libraries-path \"C:\\Users\\coloz\\Documents\\aily-project\\AIVOXSensor_sep03a\\.temp\\libraries\"",
    "test-blinker": "ts-node main.ts compile examples/Hello_WiFi.ino --verbose --board esp32:esp32:esp32s3 --libraries-path \"C:\\Users\\coloz\\Documents\\Arduino\\libraries\"",
    "clean": "rimraf dist"
  },
  "keywords": [
    "arduino",
    "cli",
    "compilation",
    "build",
    "embedded"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.5",
    "iconv-lite": "^0.7.0",
    "ora": "^5.4.1",
    "tree-sitter": "^0.20.4",
    "tree-sitter-cpp": "^0.20.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.16",
    "jest": "^29.7.0",
    "nexe": "^5.0.0-beta.4",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16"
  }
}
