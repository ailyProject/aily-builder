{
  "name": "aily-builder",
  "version": "1.1.4",
  "description": "Fast Arduino compilation CLI tool with optimized preprocessing and parallel compilation",
  "main": "dist/main.js",
  "bin": {
    "aily": "dist/main.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "bundle": "npm run build && node bundle.js",
    "bundle:minify": "npm run build && node bundle.js --minify",
    "bundle:both": "npm run build && node bundle.js --both",
    "bundle:native": "npm run build && node bundle-native.js",
    "bundle:native:minify": "npm run build && node bundle-native.js --minify",
    "bundle:native:both": "npm run build && node bundle-native.js --both",
    "package": "npm run build && node build.js",
    "start": "ts-node main.ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "arduino",
    "cli",
    "compilation",
    "build",
    "embedded"
  ],
  "author": "coloz",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@ast-grep/lang-cpp": "^0.0.4",
    "@ast-grep/napi": "^0.40.3",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.5",
    "tree-sitter": "^0.20.4",
    "tree-sitter-cpp": "^0.20.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.16",
    "esbuild": "^0.25.9",
    "jest": "^29.7.0",
    "nexe": "^5.0.0-beta.4",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16"
  }
}
